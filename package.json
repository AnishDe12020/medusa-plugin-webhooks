{
	"name": "medusa-plugin-webhooks",
	"version": "0.0.1",
	"description": "A Medusa plugin that gives webhooks functioanlity",
	"author": "AnishDe12020 <contact@anishde.dev>",
	"license": "MIT",
	"scripts": {
		"build-old": "babel src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
		"build": "babel src -d .",
		"watch": "babel -w src --out-dir . --ignore **/__tests__ --extensions \".ts,.js\"",
		"prepare": " cross-env NODE_ENV=production npm run build"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.18.10",
		"@babel/node": "^7.7.4",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/plugin-transform-instanceof": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.7.6",
		"@babel/preset-env": "^7.7.5",
		"@babel/preset-typescript": "^7.14.5",
		"@babel/register": "^7.7.4",
		"@babel/runtime": "^7.9.6",
		"babel-preset-medusa-package": "^1.1.19",
		"cross-env": "^7.0.3",
		"install-peers-cli": "^2.2.0"
	},
	"peerDependencies": {
		"@medusajs/medusa": "^1.3.1",
		"medusa-interfaces": "^1.3.3",
		"typeorm": "^0.2.36"
	},
	"repository": "https://github.com/AnishDe12020/medusa-plugin-webhooks.git",
	"keywords": [
		"medusa",
		"webhooks"
	],
	"dependencies": {
		"axios": "^1.1.2"
	}
